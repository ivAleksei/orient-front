{
  "name": "base-orient-front",
  "version": "1.0.0",
  "author": "Ivan Aleksei",
  "description": "PWA de gestão esportiva, com foco inicial no esporte de orientação",
  "homepage": "https://baseorient.ialk.com.br/",
  "private": true,
  "config": {
    "forge": {
      "makers": [
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin",
            "linux"
          ],
          "config": {}
        }
      ]
    }
  },
  "scripts": {
    "ng": "ng",
    "git-clear": "git fetch --prune",
    "merge": "git merge origin/master",
    "git-setup": "npm run git-clear && npm run merge",

    "electron": "electron ./www/main.js",
    "start:electron": "electron-forge start",
    "package:electron": "electron-forge package",
    "make:electron": "electron-forge make --arch=\"ia32,x64\"",

    "local-ip": "node local_ip.js",

    "prestart": "npm run git-setup && npm run local-ip",
    "start": "npm run start:cfap",
    "prestart:baseorient": "npm run prestart",
    "start:baseorient": "ionic serve --project=baseorient --livereload --external",
    "deploy:baseorient": "node tasks/deploy --env=prod --app=baseorient",

    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "deploy": "npm run deploy:baseorient",
    "test": "ng test",
    "lint": "ng lint",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
